<dock:DockableContent x:Class="ShareTabWin.LogPanel"
                      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                      xmlns:dock="clr-namespace:AvalonDock;assembly=AvalonDock"
                      Title="Log">

    <Grid>
        <TextBox Name="logBox" IsReadOnly="True" TextWrapping="Wrap"
                 Text="{Binding Trace, Mode=OneWay}" />
    </Grid>
</dock:DockableContent>
