<dock:DockableContent x:Class="ShareTabWin.TabsPanel"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:dock="clr-namespace:AvalonDock;assembly=AvalonDock"
        xmlns:local="clr-namespace:ShareTabWin"
        Title="Tabs">
    <Grid>
        <TreeView x:Name="treeView" ItemsSource="{x:Static local:Session.Sessions}">
            <TreeView.ItemTemplate>
                <HierarchicalDataTemplate ItemsSource="{Binding Path=Tabs}">
                    <TextBlock Text="{Binding Path=DisplayName}"></TextBlock>
                </HierarchicalDataTemplate>
            </TreeView.ItemTemplate>
        </TreeView>
    </Grid>
</dock:DockableContent>
